<script>
  document.addEventListener("DOMContentLoaded", function () {
    // Get the current URL
    let currenturl = window.location.pathname;

    // Check if the URL matches the desired endpoint
    if (
      currenturl === "/test-cut-run-complete-guide/" ||
      currenturl === "/about/management/"
    ) {
      let rmCont = document.querySelector(".remove-container");
      // If the element is found, remove the specified classes
      if (rmCont) {
        rmCont.classList.remove("container");
        rmCont.classList.remove("container-small");
      }
    }
  });

  $(".scrollTo").on("click", function (e) {
    e.preventDefault();
    var target = $(this).attr("href");
    $("html, body").animate(
      {
        scrollTop: $(target).offset().top - 130,
      },
      {
        duration: 2000,
        complete: function () {
          // Add a shadow to the targeted div
          $(target).css("box-shadow", "0 1px 8px 3px rgb(70 152 203 / 30%)");

          // Remove the shadow after a certain time (e.g., 2 seconds)
          setTimeout(function () {
            $(target).css("box-shadow", "none");
          }, 2000);
        },
      }
    );
  });
</script>

<script>

  function getUTMParameter(name) {

    let result = new RegExp('[\?&]' + name + '=([^&#]*)').exec(window.location.href);

    return result === null ? '' : decodeURIComponent(result[1]) || '';

  }



  document.addEventListener("DOMContentLoaded", function() {

    document.querySelector("input[name='UTM_Campaign__c']").value = getUTMParameter('utm_campaign');

    document.querySelector("input[name='UTM_Content__c']").value = getUTMParameter('utm_content');

    document.querySelector("input[name='UTM_Medium__c']").value = getUTMParameter('utm_medium');

    document.querySelector("input[name='UTM_Source__c']").value = getUTMParameter('utm_source');

    document.querySelector("input[name='UTM_Term__c']").value = getUTMParameter('utm_term');

  });

</script>
